{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false,
    "timestamps": true
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "user_activity_event_group_id": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::user-activity-event-group.user-activity-event-group",
      "mappedBy": "user_id"
    },
    "activity_event_id": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::activity-event-group.activity-event-group",
      "mappedBy": "user_creator_id"
    },
    "user_favorite_place_id": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::activity-event-group.activity-event-group",
      "mappedBy": "user_id"
    },
    "users_favorite_place_id": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::users-favorite-place.users-favorite-place",
      "mappedBy": "user_id"
    }
  }
}
